#kdl_name = BBT_get
Data.BBT.get <k> <v> (cmp: k -> k -> Cmp) (key: k) (map: Data.BBT k v): Maybe v {
  match Data.BBT map {
    tip => Maybe.none
    bin (map.key @ key) val left right size => 
      let comp = cmp key map.key
      match Cmp comp {
        ltn => (Data.BBT.get k v cmp key left)
        eql => Maybe.some val
        gtn => (Data.BBT.get k v cmp key right)
      }
  }
}
